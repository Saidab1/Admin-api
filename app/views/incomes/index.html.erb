<p id="notice"><%= notice %></p>

<h1>Incomes</h1>

<table>
  <thead>
    <tr>
      <th>Invoice number</th>
      <th>Ruc number</th>
      <th>Total amount</th>
      <th>Company name</th>
      <th>Description</th>
      <th>Paid date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.invoice_number %></td>
        <td><%= income.ruc_number %></td>
        <td><%= income.total_amount %></td>
        <td><%= income.company_name %></td>
        <td><%= income.description %></td>
        <td><%= income.paid_date %></td>
        <td><%= link_to 'Show', income %></td>
        <td><%= link_to 'Edit', edit_income_path(income) %></td>
        <td><%= link_to 'Destroy', income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income', new_income_path %>
